  name = Premysl Donat

[color]
  ui = auto

[alias]
  rebx = rebase -i HEAD~10
  rhi = reset --hard HEAD~1
  rsi = reset --soft HEAD~1
  rhx = reset --hard HEAD~10
  rsx = reset --soft HEAD~10
  rebc = rebase --continue
  merc = merge --continue
  ci = commit
  br = branch
  co = checkout
  lol = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
  pm = pull origin master
  s = status
  amend = commit --amend --no-edit

[branch]
  autosetuprebase = always

[push]
  default = current
[core]
	excludesfile = ~/.gitignore
