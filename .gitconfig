[alias]
    st = status
    rx = rebase -i HEAD~10
    ri = rebase -i HEAD~1
    ci = commit
    br = branch
    co = checkout
    df = diff
    lg = log -p
    who = shortlog -s --
    lol = log --pretty=oneline --abbrev-commit --graph --decorate
[branch]
	autosetuprebase = always
